{
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-s lint type-check test:unit build-only",
    "test": "run-s lint type-check test:unit",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/ --silent --run",
    "test:unit:dev": "vitest --environment jsdom --root src/ --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/google.maps": "^3.58.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.19.54",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^21.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.79.4",
    "start-server-and-test": "^1.15.4",
    "vite": "^4.5.5",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.27",
    "vue3-google-map": "^0.18.0"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "express": "^4.21.0",
    "typescript": "^5.6.2",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.2"
  }
}
